@Article{Pianegiani2008,
  Title                    = {Energy-Efficient Signal Classification in Ad Hoc Wireless Sensor Networks},
  Author                   = {Pianegiani, F. and Hu Mingqing and Boni, A. and Petri, D.},
  Journal                  = IEEE_J_IM,
