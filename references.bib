@Article{Linares-Barranco2006,
  Title                    = {On algorithmic rate-coded AER generation},
  Author                   = {Linares-Barranco, A. and Jimenez-Moreno, G. and Linares-Barranco, B. and Civit-Balcells, A.},
  Journal                  = IEEE_J_NN,
