@Article{Rochas2003,
  Title                    = {First fully integrated {2-D} array of single-photon detectors in standard {CMOS} technology},
  Author                   = {Rochas, A. and Gosch, M. and Serov, A. and Besse, P.A. and Popovic, R.S. and Lasser, T. and Rigler, R.},
