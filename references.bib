@Article{Bellazzini2006a,
  Title                    = {Direct reading of charge multipliers with a self-triggering {CMOS} analog chip with 105k pixels at 50um pitch},
  Author                   = {R. Bellazzini and G. Spandre and M. Minuti and L. Baldini and A. Brez and F. Cavalca and L. Latronico and N. Omodei and M.M. Massai and C. Sgroâ and E. Costa and P. Soffitta and F. Krummenacher and R. de Oliveira},
