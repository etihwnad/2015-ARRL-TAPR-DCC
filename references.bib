@Article{Yang1999,
  Title                    = {A 640 times;512 CMOS image sensor with ultrawide dynamic range floating-point pixel-level ADC},
  Author                   = {Yang, D.X.D. and Gamal, A.E. and Fowler, B. and Tian, H.},
  Journal                  = IEEE_J_JSSC,
  Year                     = {1999},

