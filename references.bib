  Intrahash                = {0111f5379630a5df49fe4dae5fce7e08},
  ISSN                     = {1751-8628},
  Keywords                 = {networkablesensors, wirelesssensornetworks, wirelesssensornetworksMEMS, microelectromechanicalsystem, routingprotocol, microsensors, protocols, datatransmission, directed-diffusion, routingprotocols, telecommunicationnetworkreliability, radiofrequencytechnology, reliableandenergyefficientprotocol},
  Owner                    = {etihwnad},
  Timestamp                = {2010-01-19 16:22:04}
