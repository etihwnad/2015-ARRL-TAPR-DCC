@Article{Autiero2005,
  Title                    = {Experimental study on in vivo optical and radionuclide imaging in small animals},
  Author                   = {Autiero, M. and Celentano, L. and Cozzolino, R. and Laccetti, P. and Marotta, M. and Mettivier, G. and Montesi, M.C. and Riccio, P. and Roberti, G. and Russo, P.},
